             CHGPFM     FILE(TOUCH/QSQLSRC) MBR(TOUCHCFG) SRCTYPE(SQL)
             IF         COND(&FILE *EQ '*YES') THEN(DO)
             RUNSQLSTM  SRCFILE(TOUCH/QSQLSRC) SRCMBR(TOUCHTRX) +
                          COMMIT(*NONE) ERRLVL(30)
             ENDDO
             /* Service programs */
